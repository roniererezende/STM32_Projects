#include "bsp.h"

uint16_t count_millissecond = 0;

// Função de Callback da Interrupção
void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim)
{
	count_millissecond++;
	if(count_millissecond > 1999)
	{
		bsp.state = run;
	}
}
